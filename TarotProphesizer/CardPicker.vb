    Private Sub CardPickerButton_Click(sender As Object, e As EventArgs) Handles CardPickerButton.Click
        If CardSelected <> 3 Then
            MsgBox("You have to select 3 cards!")
            Exit Sub
        End If
        Prophecy.Show()
        Me.Close()
    End Sub
